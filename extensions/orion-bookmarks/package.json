{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "orion-bookmarks",
  "title": "Orion Bookmarks",
  "description": "Search your Orion bookmarks",
  "icon": "orion-icon.png",
  "author": "plonq",
  "categories": [
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Orion Bookmarks",
      "description": "Search bookmarks from the Orion browser",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.2",
    "@raycast/utils": "^1.3.1",
    "bplist-parser": "^0.3.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
